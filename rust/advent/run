#!/bin/sh

datadir=../../data

cargo run -q 01a <$datadir/advent01.txt
cargo run -q 01b <$datadir/advent01.txt
cargo run -q 02a <$datadir/advent02.txt
cargo run -q 02b <$datadir/advent02.txt
cargo run -q 03a <$datadir/advent03.txt
cargo run -q 03b <$datadir/advent03.txt
cargo run -q 04a <$datadir/advent04.txt
cargo run -q 04b <$datadir/advent04.txt
cargo run -q 05a <$datadir/advent05.txt
cargo run -q 05b <$datadir/advent05.txt
cargo run -q 06a <$datadir/advent06.txt
cargo run -q 06b <$datadir/advent06.txt
cargo run -q 07a <$datadir/advent07.txt
cargo run -q 07b <$datadir/advent07.txt
cargo run -q 08a <$datadir/advent08.txt
echo "08b -> 14265111103729 (from java, and slow)"
cargo run -q 09a <$datadir/advent09.txt
cargo run -q 09b <$datadir/advent09.txt
cargo run -q 10a <$datadir/advent10.txt
cargo run -q 10b <$datadir/advent10.txt
cargo run -q 11a <$datadir/advent11.txt
cargo run -q 11b <$datadir/advent11.txt
cargo run -q 12a <$datadir/advent12.txt
echo "12b fail"
cargo run -q 13a <$datadir/advent13.txt
cargo run -q 13b <$datadir/advent13.txt
cargo run -q 14a <$datadir/advent14.txt
cargo run -q 14b <$datadir/advent14.txt
cargo run -q 15a <$datadir/advent15.txt
cargo run -q 15b <$datadir/advent15.txt
cargo run -q 16a <$datadir/advent16.txt
echo "17a fail"
echo "17b fail"
cargo run -q 16b <$datadir/advent16.txt
cargo run -q 18a <$datadir/advent18.txt
