#!/bin/sh

datadir=../../data

cargo run -q 01a <$datadir/advent1.txt
cargo run -q 01b <$datadir/advent1.txt
cargo run -q 02a <$datadir/advent2.txt
cargo run -q 02b <$datadir/advent2.txt
cargo run -q 03a <$datadir/advent3.txt
cargo run -q 03b <$datadir/advent3.txt
cargo run -q 04a <$datadir/advent4.txt
cargo run -q 04b <$datadir/advent4.txt
cargo run -q 05a <$datadir/advent5.txt
cargo run -q 05b <$datadir/advent5.txt
cargo run -q 06a <$datadir/advent6.txt
